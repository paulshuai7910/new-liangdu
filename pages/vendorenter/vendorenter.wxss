@import '../../style/icon-font.wxss';
.from_box{width: 720rpx;margin: 30rpx auto;border-radius: 20rpx;background-color: #fff;}
.from_box .one{margin-top: 18rpx;}
.from_content {
  border-radius: 20rpx;
}

.section {
  position: relative;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  line-height: 1.4;
  padding: 24rpx 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
}

.from_box {
  margin-bottom: 10rpx;
}

.input__combine {
  position: relative;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  line-height: 1.4;
  padding: 24rpx 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;

}

.input__combine::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 200%;
  height: 200%;
  transform: scale(0.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-bottom-width: 1px;
}

.input__label {
  min-width: 163rpx;
  height: 46rpx;
  line-height: 46rpx;
  color: #333;
}
.input__combine--nob {
  position: relative;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  line-height: 1.4;
  padding: 24rpx 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
}
.input__combine--nob .chose_val{
  width: 480rpx;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;
}
.checkbox{padding: 0 30rpx;box-sizing: border-box;display: flex;flex-wrap: wrap;justify-content: space-around;}
.checkbox label{line-height: 80rpx;width: 200rpx;}
.from_content picker{
  font-size:30rpx;
  color:#333;
  display: flex;
  align-items: center;
}
.input__combine_cam{display: flex;flex-flow: row nowrap;align-items: center;line-height: 1.4;justify-content: space-around;padding-top: 30rpx;}
.input__combine_cam .input_cam{
  width: 120rpx;height: 54rpx;border-radius: 5rpx;border: 2rpx solid #ededed;line-height: 54rpx;text-align: center;margin-bottom: 15rpx;
}
.input__combine_cam .input_icon{width: 200rpx;height: 200rpx;line-height: 200rpx;text-align: center;border: 2rpx solid #ededed;border-radius: 20rpx;}
.input__combine_cam .input_icon .iconfont{font-size: 80rpx;color: #666;}
.input__combine_cam .input_lab{line-height: 90rpx;font-size: 26rpx;color: #666;}

.concet{display: flex;justify-content: flex-start;padding:24rpx 30rpx;font-size: 28rpx;color: #333;align-items: center;}
.concet label{margin-right: 10rpx;}
.from_swiper{padding: 0rpx 30rpx 10rpx 30rpx;}
 .swiper_title{font-size: 28rpx;line-height: 55rpx;}
 .swiper_icon{width: 156rpx;height: 167rpx;border: 2rpx solid #ededed;line-height: 167rpx;text-align: center;}
 .swiper_icon .iconfont{font-size: 90rpx;color: #ccc;}
 .input__combine_detail{display: flex;justify-content: space-between;align-items: center;}
 .input__combine_detail .input_num{color: #999;line-height: 55rpx;}
 .from_detail{padding-bottom: 10rpx;padding:0rpx 30rpx 10rpx 30rpx;}
 .from_phone{justify-content: space-between;display: flex;align-items: center;height: 82rpx;padding: 0 30rpx;}
 .input__combine_phone{display: flex;justify-content: flex-start;align-items: center;}
 .input_cam{color: #333;}
 .input_vnum{width: 170rpx;height: 70rpx;background-color: #da4f50;color: #fff;
  text-align: center;line-height: 80rpx;border-radius: 5rpx;}
  .from_enter{padding: 0 30rpx; border-radius: 20rpx;background-color: #fff;}
.from_enter .swiper_title{line-height: 80rpx;}
 .input_radio{display: flex;justify-content: flex-start;align-items: center;}
 .input_radio .input_moy{line-height: 85rpx;}
 .apply{width: 750rpx;height: 80rpx;line-height: 80rpx;background-color: #db384c;text-align: center;color: #fff;font-size: 28rpx;}
 .agreebox{height: 120rpx;width: 100%;padding: 0 30rpx;display: flex;justify-content: flex-start;align-items: center;}
 .agreebox .input_reade{font-size: 28rpx;color: #666;}
 .agreebox .input_agree{color: #ecb5b6;font-size: 28rpx;}
 .add_facility {
  background-color: #1aad19;
  color: #fff;
  font-size: 24rpx;
  display: flex;
  justify-items: center;
  align-items: center;
  padding: 0 10rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 8rpx;
  margin-bottom: 15rpx;
}

.add_facility .iconfont {
  font-size: 24rpx;
}

.facility_item {
  height: 50rpx;
  line-height: 50rpx;
  border: 1rpx solid #1aad19;
  font-size: 24rpx;
  padding: 0 10rpx;
  border-radius: 8rpx;
  color: #333;
  min-width: 80rpx;
  text-align: center;
  box-sizing: border-box;
  margin-right: 15rpx;
  position: relative;
  margin-bottom: 15rpx;
}

.facility_item .iconfont {
  position: absolute;
  color: #db384c;
  top: -22rpx;
  right: -12rpx;
}

.facility_list {
  padding: 0 20rpx 20rpx 20rpx;
}
.qrcode_box{
  width: 200rpx;
  height: 200rpx;
  border: 1rpx solid #888;
  font-size: 24rpx;
  display: flex;
  color: #888;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}
.qrcode_box .iconfont{
  font-size: 60rpx;
}
.qrcode_box label{
  line-height: 40rpx;
}
.textarea_box{
  font-size: 26rpx;
  padding:10rpx 30rpx;text-indent: 2em;
}
.textarea_box textarea{
  height: 150rpx;
  width: 100%;
  border-bottom: 1rpx solid #d5d5d5;
}
